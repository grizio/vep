package vep.app.common.verifications

import vep.app.common.types.Period

def periodIsIncluded(period: Period, reference: Period): Boolean => {
  period.start.isAfterOrEqual(reference.start) && period.end.isBeforeOrEqual(reference.end)
}

def dateIsIncluded(date: Date, reference: Period): Boolean => {
  date.isAfterOrEqual(reference.start) && date.isBeforeOrEqual(reference.end)
}