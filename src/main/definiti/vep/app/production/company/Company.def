package vep.app.production.company

import vep.app.common.verifications.IsNonBlank

type Company {
  id: String verifying IsNonBlank("Veuillez renseigner l'identifiant de la companie")
  name: String verifying IsNonBlank("Veuillez renseigner le nom")
  address: String verifying IsNonBlank("Veuillez renseigner l'adresse")
  isVep: Boolean
  content: String verifying IsNonBlank("Veuillez renseigner le contenu")
}

type CompanyCreation {
  name: String verifying IsNonBlank("Veuillez renseigner le nom")
  address: String verifying IsNonBlank("Veuillez renseigner l'adresse")
  isVep: Boolean
  content: String verifying IsNonBlank("Veuillez renseigner le contenu")
}